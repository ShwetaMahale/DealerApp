<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    app:layout_constraintCircleRadius="@dimen/_300sdp"
    android:background="@color/transparent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <com.mwbtech.dealer_register.Utils.Progressbar.ProgressCustom
        android:id="@+id/myProgBar"
        app:layout_constraintCircleRadius="@dimen/_100sdp"
        app:barType="@drawable/delay_less"
        android:background="@color/transparent"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:layout_below="@id/progressBar"
            android:padding="8dp"
            android:layout_marginTop="8dp"
            android:text=""
            android:textSize="16sp"
            android:id="@+id/txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </com.mwbtech.dealer_register.Utils.Progressbar.ProgressCustom>
</androidx.constraintlayout.widget.ConstraintLayout>